{
  "name": "@kcuf/fork-console-base-dialog-core",
  "version": "0.0.8",
  "description": "A fork of @alicloud/console-base-dialog-core.",
  "keywords": [],
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
