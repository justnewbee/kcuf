{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "esnext",
      "dom"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "lib"
  }
}
