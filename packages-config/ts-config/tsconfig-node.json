{
  "compilerOptions": {
    "outDir": "lib",
    "module": "commonjs",
    "target": "esnext",
    "lib": [
      "es6",
      "es2017.string"
    ],
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true
  }
}
